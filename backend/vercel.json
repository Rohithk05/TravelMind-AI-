{
    "version": 2,
    "builds": [
        {
            "src": "app/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/main.py"
        }
    ],
    "env": {
        "GEMINI_API_KEY": "@gemini_api_key",
        "GROQ_API_KEY": "@groq_api_key",
        "SUPABASE_URL": "@supabase_url",
        "SUPABASE_ANON_KEY": "@supabase_anon_key",
        "SECRET_KEY": "@secret_key"
    }
}